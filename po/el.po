#: ../anaconda:98
msgid "Starting VNC..."
msgstr "Έναρξη VNC..."

#: ../anaconda:133
#, c-format
msgid "%s %s installation on host %s"
msgstr "εγκατάσταση %s %s στο σύστημα %s"

#: ../anaconda:135
#, c-format
msgid "%s %s installation"
msgstr "εγκατάσταση %s %s"

#: ../anaconda:155
